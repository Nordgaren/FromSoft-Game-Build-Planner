<Window x:Class="FromSoft_Game_Build_Planner.DarkSouls1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FromSoft_Game_Build_Planner"
        mc:Ignorable="d"
        Title="Dark Souls" Height="450" Width="800">

    <Grid>
        <ComboBox x:Name="cmbWeapon" HorizontalAlignment="Left" Margin="17,37,0,0" VerticalAlignment="Top" Width="139" SelectionChanged="cmbWeapon_SelectionChanged">
            <ComboBox.GroupStyle>
                <GroupStyle>
                    <GroupStyle.HeaderTemplate>
                        <DataTemplate>
                            <TextBlock FontWeight="Bold" FontSize="14" Text="{Binding Name}"/>
                        </DataTemplate>
                    </GroupStyle.HeaderTemplate>
                </GroupStyle>
            </ComboBox.GroupStyle>
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Name}"/>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>
        <ComboBox x:Name="cmbInfusion" HorizontalAlignment="Left" Margin="17,64,0,0" VerticalAlignment="Top" Width="86" SelectionChanged="cmbInfusion_SelectionChanged"/>
        <local:IntegerUpDown x:Name="nudVit" Margin="719,104,6,286" ValueChanged="nud_ValueChanged" TabIndex="1"/>
        <local:IntegerUpDown x:Name="nudAtt" Margin="719,126,6,264" ValueChanged="nud_ValueChanged" TabIndex="2"/>
        <local:IntegerUpDown x:Name="nudEnd" Margin="719,148,6,242" ValueChanged="nud_ValueChanged" TabIndex="3"/>
        <local:IntegerUpDown x:Name="nudStr" Margin="719,170,6,220" ValueChanged="nud_ValueChanged" TabIndex="4"/>
        <local:IntegerUpDown x:Name="nudDex" Margin="719,192,6,198" ValueChanged="nud_ValueChanged" TabIndex="5"/>
        <local:IntegerUpDown x:Name="nudRes" Margin="719,214,6,176" ValueChanged="nud_ValueChanged" TabIndex="6"/>
        <local:IntegerUpDown x:Name="nudInt" Margin="719,236,6,154" ValueChanged="nud_ValueChanged" TabIndex="7"/>
        <local:IntegerUpDown x:Name="nudFai" Margin="719,258,6,132" ValueChanged="nud_ValueChanged" TabIndex="8"/>
        <Label x:Name="lblVit" Content="Vitality" HorizontalAlignment="Left" Margin="652,103,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblAtt" Content="Attunement" HorizontalAlignment="Left" Margin="651,125,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblEnd" Content="Endurance" HorizontalAlignment="Left" Margin="651,147,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblStr" Content="Strength" HorizontalAlignment="Left" Margin="651,169,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblDex" Content="Dexterity" HorizontalAlignment="Left" Margin="651,191,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblRes" Content="Resistance" HorizontalAlignment="Left" Margin="651,213,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblInt" Content="Intelligence" HorizontalAlignment="Left" Margin="652,235,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblFai" Content="Faith" HorizontalAlignment="Left" Margin="652,257,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtSoulLevel" HorizontalAlignment="Left" Margin="722,86,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="46" IsEnabled="False"/>
        <Label x:Name="lblSL" Content="Soul Level" HorizontalAlignment="Left" Margin="651,83,0,0" VerticalAlignment="Top" IsTabStop="True"/>
        <ComboBox x:Name="cmbClass" HorizontalAlignment="Left" Margin="656,58,0,0" VerticalAlignment="Top" Width="126" SelectionChanged="cmbClass_SelectionChanged"/>
        <local:IntegerUpDown x:Name="nudUpgrade" Margin="106,64,621,326" ValueChanged="nud_ValueChanged"/>
        <TextBox x:Name="txtPysAR" HorizontalAlignment="Left" Margin="316,75,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54"/>
        <Label x:Name="lblPhysAR" Content="Phys AR" HorizontalAlignment="Left" Margin="375,70,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtMagAR" HorizontalAlignment="Left" Margin="316,101,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54"/>
        <Label x:Name="lblMagAR" Content="Mag AR" HorizontalAlignment="Left" Margin="375,96,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtFireAR" HorizontalAlignment="Left" Margin="316,127,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54"/>
        <Label x:Name="lblMagAR_Copy" Content="Fire AR" HorizontalAlignment="Left" Margin="375,122,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtLightAR" HorizontalAlignment="Left" Margin="316,153,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54"/>
        <Label x:Name="lblLightAR" Content="Lightning AR" HorizontalAlignment="Left" Margin="375,148,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="txtTotalAR" HorizontalAlignment="Left" Margin="316,48,0,0" Text="0" TextWrapping="Wrap" VerticalAlignment="Top" Width="54"/>
        <Label x:Name="lblTotalAR" Content="Total AR" HorizontalAlignment="Left" Margin="375,45,0,0" VerticalAlignment="Top"/>
        <local:IntegerUpDown x:Name="nudHumanity" Margin="719,31,6,359" ValueChanged="nud_ValueChanged" TabIndex="1"/>
        <Label x:Name="lblHumanity" Content="Humanity" HorizontalAlignment="Left" Margin="652,30,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
